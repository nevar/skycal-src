{ "globals":
    { "$": true
    , "Vue": true
    }
, "parser": "babel-eslint"
, "ecmaFeatures":
    { "blockBindings": true
    , "arrowFunctions": true
    , "classes": true
    , "defaultParams": true
    , "modules": true
    , "templateStrings": true
    }
, "rules":
    { "indent": [2, 4]
    , "quotes": [2, "single"]
    , "linebreak-style": [2, "unix"]
    , "semi": [2, "always"]
    , "no-var": [2]
    }
, "env":
    { "es6": true
    , "node": true
    , "browser": true
    }
, "extends": "eslint:recommended"
}
